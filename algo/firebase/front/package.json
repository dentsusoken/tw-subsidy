    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "mocha": "^10.0.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8",
    "ts-mocha": "^10.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "4.8.3"
